{
   "services":[
      {
         "object-class":"InfoPoint",
         "base-class":"BERequest",
         "mapping":{
            "infoPointId":"id"
         },
         "methods":[
            {
               "name":"RetrieveInfoPoint",
               "parameters":[
                  {
                     "name":"infoPointId",
                     "type":"numeric",
                     "mandatory":true
                  }
               ],
               "constants":[

               ],
               "environment":"BE_GET",
               "endpoint":"/infopoint/:infoPointId",
               "response":"InfoPointSingleResponse"
            },
            {
               "name":"RetrieveInfoPoints",
               "parameters":[

               ],
               "constants":[

               ],
               "environment":"BE_GET",
               "endpoint":"/infopoint",
               "response":"InfoPointListResponse"
            }
         ]
      }
   ],
   "responses":[
      {
         "object-class":"InfoPointListResponse",
         "base-class":"BEResponse",
         "mapping":{
            "data":{
               "property":"infoPoints",
               "isArray": true,
               "object-class":"InfoPointItem"
            }
         }
      },
      {
         "object-class":"InfoPointSingleResponse",
         "base-class":"BEResponse",
         "mapping":{
            "data":{
               "property":"infoPoint",
               "object-class":"InfoPointItem"
            }
         }
      },
      {
         "object-class":"InfoPointItem",
         "base-class":"BEResponse",
         "mapping":{
            "id":{
               "property":"identifier",
               "type":"string"
            },
            "name":{
               "property":"title",
               "type":"string"
            },
                "contentType":{
                "property":"contentType",
                "type":"string"
                },
            "address":{
               "property":"address",
               "type":"string"
            },
            "latitude":{
               "property":"latitude",
               "type":"numeric"
            },
            "longitude":{
               "property":"longitude",
               "type":"numeric"
            },
            "email":{
               "property":"email",
               "type":"string"
            },
            "telephone":{
               "property":"telephone",
               "type":"string"
            },
            "images":{
               "property":"images",
               "isArray": true,
               "object-class":"CaptionImage"
            },
            "mainImage":{
               "property":"mainImage",
               "object-class":"NSURL",
               "no-import": true
            },
            "openingTimes":{
               "property":"openingTimes",
               "isArray": true,
               "object-class":"OpeningTime"
            },
            "openingTimesNotes":{
               "property":"openingTimesNotes",
               "type":"string"
            }
         }
      }
   ]
}
