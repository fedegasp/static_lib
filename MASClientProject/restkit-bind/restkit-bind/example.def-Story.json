{
   "services":[
      {
         "object-class":"Stories",
         "base-class":"BERequest",
         "mapping":{
         },
         "methods":[
            {
               "name":"Retrieve",
               "parameters":[
               ],
               "constants":[
               ],
               "environment":"BE_GET",
               "endpoint":"/story",
               "response":"StoriesResponse"
            }
         ]
      }
   ],
   "responses":[
      {
         "object-class":"StoriesResponse",
         "base-class":"BEResponse",
         "mapping":{
            "data":{
               "property":"stories",
               "isArray": true,
               "object-class":"StoriesResponseItem"
            }
         }
      },
      {
         "object-class":"StoriesResponseItem",
         "base-class":"BEResponse",
         "mapping":{
            "id":{
               "property":"identifier",
               "type":"string"
            },
            "name":{
               "property":"name",
               "type":"string"
            },
            "subtitle":{
               "property":"subtitle",
               "type":"string"
            },
            "bigImage":{
               "property":"bigImage",
               "type":"string"
            },
            "videoUrl":{
               "property":"videoUrl",
               "type":"string"
            },
            "modules":{
               "property":"modules",
               "isArray": true,
               "object-class":"StoryModule"
            }
         }
      },
      {
         "object-class":"StoryModule",
         "base-class":"BEResponse",
         "mapping":{
            "moduleId":{
               "property":"identifier",
               "type":"string"
            },
            "title":{
               "property":"title",
               "type":"string"
            },
            "subtitle":{
               "property":"subtitle",
               "type":"string"
            },
            "image":{
               "property":"image",
               "type":"string"
            },
            "content":{
               "property":"content",
               "type":"string"
            },
            "quote":{
               "property":"quote",
               "type":"string"
            },
            "quoteAuthor":{
               "property":"quoteAuthor",
               "type":"string"
            },
            "images":{
               "property":"images",
               "isArray": true,
               "object-class": "NSURL",
               "no-import": true
            },
            "pois":{
               "property":"poiList",
               "isArray": true,
               "object-class":"Poi"
            }
         }
      }
   ]
}
