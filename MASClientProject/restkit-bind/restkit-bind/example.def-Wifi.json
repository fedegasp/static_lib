{
   "services":[
      {
         "object-class":"Wifi",
         "base-class":"BERequest",
         "mapping":{
            "wifiId":"id"
         },
         "methods":[
            {
               "name":"GetWifiInfo",
               "parameters":[
                  {
                     "name":"wifiId",
                     "type":"numeric",
                     "mandatory":true
                  }
               ],
               "constants":[

               ],
               "environment":"BE_GET",
               "endpoint":"/wifi/:wifiId",
               "response":"WifiSingleResponse"
            },
            {
               "name":"GetWifiList",
               "parameters":[
               ],
               "constants":[

               ],
               "environment":"BE_GET",
               "endpoint":"/wifi",
               "response":"WifiListResponse"
            }
         ]
      }
   ],
   "responses":[
      {
         "object-class":"WifiListResponse",
         "base-class":"BEResponse",
         "mapping":{
            "data":{
               "property":"hotspots",
               "isArray": true,
               "object-class":"WifiHotspot"
            }
         }
      },
      {
         "object-class":"WifiSingleResponse",
         "base-class":"BEResponse",
         "mapping":{
            "data":{
               "property":"hotspot",
               "object-class":"WifiHotspot"
            }
         }
      },
      {
         "object-class":"WifiHotspot",
         "base-class":"BEResponse",
         "mapping":{
            "id":{
               "property":"identifier",
               "type":"string"
            },
            "contentType":{
                "property":"contentType",
                "type":"string"
            },
            "name":{
               "property":"name",
               "type":"string"
            },
            "address":{
               "property":"address",
               "type":"string"
            },
            "latitude":{
               "property":"latitude",
               "type":"numeric"
            },
            "longitude":{
               "property":"longitude",
               "type":"numeric"
            }
         }
      }
   ]
}
