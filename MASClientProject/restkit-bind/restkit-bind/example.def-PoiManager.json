{
   "services":[
      {
         "object-class":"PoiManager",
         "base-class":"BERequest",
         "mapping":{
            "poiId":"id"
         },
         "methods":[
            {
               "name":"GetPoiDetails",
               "parameters":[
                  {
                     "name":"poiId",
                     "type":"string",
                     "mandatory":true
                  }
               ],
               "constants":[

               ],
               "environment":"BE_GET",
               "endpoint":"/poi/:poiId",
               "response":"PoiSingleResponse"
            },
            {
               "name":"GetPoiList",
               "parameters":[
               ],
               "constants":[

               ],
               "environment":"BE_GET",
               "endpoint":"/poi",
               "response":"PoiListResponse"
            }
         ]
      }
   ],
   "responses":[
      {
         "object-class":"PoiSingleResponse",
         "base-class":"BEResponse",
         "mapping":{
            "data":{
               "property":"poi",
               "object-class":"Poi"
            }
         }
      },
      {
         "object-class":"PoiListResponse",
         "base-class":"BEResponse",
         "mapping":{
            "data":{
               "property":"poiList",
               "object-class":"Poi"
            }
         }
      }
   ]
}
