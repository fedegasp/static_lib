{
   "services":[
      {
         "object-class":"TAvailability",
         "forward":["TicketHoldPoi"],
         "base-class":"BERequest",
         "mapping":{
            "start":"start",
            "end":"end",
            "tickets":"tickets"
         },
         "methods":[
            {
               "name":"retrieve",
               "parameters":[
                  {
                     "name":"start",
                     "type":"string",
                     "mandatory":true
                  },
                  {
                     "name":"end",
                     "type":"string",
                     "mandatory":true
                  },
                  {
                     "name":"tickets",
                     "type":"TicketHoldPoi*",
                     "mandatory":true
                  }
               ],
               "constants":[

               ],
               "environment":"BE",
               "endpoint":"/availability",
               "response":"AvailabilityResponse"
            }
         ]
      }
   ],
   "responses":[
      {
         "object-class":"AvailabilityResponse",
         "base-class":"BEResponse",
         "mapping":{
            "data.months":{
               "property":"months",
               "isArray": true,
               "object-class":"AvailabilityMonth"
            }
         }
      },
      {
         "object-class":"AvailabilityMonth",
         "base-class":"BEResponse",
         "mapping":{
            "name":{
               "property":"name",
               "type":"string"
            },
            "days":{
               "property":"days",
               "isArray": true,
               "object-class":"AvailabilityDay"
            }
         }
      },
      {
         "object-class":"AvailabilityDay",
         "base-class":"BEResponse",
         "mapping":{
            "name":{
               "property":"name",
               "type":"string"
            },
            "hasCapacity":{
               "property":"hasCapacity",
               "type":"numeric"
            },
            "slots":{
               "property":"slots",
               "isArray": true,
               "object-class":"AvailabilitySlot"
            }
         }
      },
      {
         "object-class":"AvailabilitySlot",
         "base-class":"BEResponse",
         "mapping":{
            "name":{
               "property":"name",
               "type":"string"
            },
            "hasCapacity":{
               "property":"hasCapacity",
               "type":"numeric"
            },
            "tickets":{
               "property":"tickets",
               "isArray": true,
               "object-class":"AvailabilityTicket"
            }
         }
      },
      {
         "object-class":"AvailabilityTicket",
         "base-class":"BEResponse",
         "mapping":{
            "groupName":{
               "property":"groupName",
               "type":"string"
            },
            "language":{
               "property":"language",
               "type":"string"
            },
            "free":{
               "property":"free",
               "type":"numeric"
            },
            "hasGuide":{
               "property":"hasGuide",
               "type":"numeric"
            }
         }
      }
   ]
}
