{
   "services":[
      {
         "object-class":"Auth",
         "base-class":"BERequest",
         "mapping":{
            "name":"name",
            "surname":"surname",
            "mail":"mail",
            "nation":"nation",
            "mobile":"mobile",
            "birth":"birth",
            "address":"address",
            "city":"city",
            "password":"pass",
            "oldPass":"oldPass",
            "nuovaPass":"newPass",
            "user":"user"
         },
         "methods":[
            {
               "name":"Register",
               "parameters":[
                  {
                     "name":"name",
                     "type":"string",
                     "mandatory":true
                  },
                  {
                     "name":"surname",
                     "type":"string",
                     "mandatory":true
                  },
                  {
                     "name":"mail",
                     "type":"string",
                     "mandatory":true
                  },
                  {
                     "name":"nation",
                     "type":"string",
                     "mandatory":true
                  }
               ],
               "constants":[

               ],
               "environment":"BE",
               "endpoint":"/user/register",
               "response":"BEResponse"
            },
            {
               "name":"Login",
               "parameters":[
                  {
                     "name":"mail",
                     "type":"string",
                     "mandatory":true
                  },
                  {
                     "name":"password",
                     "type":"string",
                     "mandatory":true
                  }
               ],
               "constants":[

               ],
               "environment":"BE",
               "endpoint":"/user/login",
               "response":"AuthResponse"
            },
            {
               "name":"Logout",
               "parameters":[
               ],
               "constants":[
               ],
               "environment":"BE_GET",
               "endpoint":"/user/logout",
               "response":"BEResponse"
            },
            {
               "name":"GetProfile",
               "parameters":[

               ],
               "constants":[

               ],
               "environment":"BE_GET",
               "endpoint":"/user",
               "response":"AuthResponse"
            },
            {
               "name":"UpdateProfile",
               "parameters":[
                  {
                     "name":"user",
                     "type":"BEUser *",
                     "mandatory":false
                  }
               ],
               "constants":[

               ],
               "environment":"BE_PUT",
               "endpoint":"/user",
               "local-mangling":[
                  {
                     "environment-name":"STUB",
                     "endpoint-mangling":"%@/put"
                  }
               ],
               "response":"AuthResponse"
            },
            {
               "name":"PasswordRecover",
               "parameters":[
                  {
                     "name":"mail",
                     "type":"string",
                     "mandatory":true
                  }
               ],
               "constants":[

               ],
               "environment":"BE",
               "endpoint":"/user/password/recover",
               "response":"BEResponse"
            },
            {
               "name":"PasswordChange",
               "parameters":[
                  {
                     "name":"oldPass",
                     "type":"string",
                     "mandatory":true
                  },
                  {
                     "name":"nuovaPass",
                     "type":"string",
                     "mandatory":true
                  }
               ],
               "constants":[

               ],
               "environment":"BE",
               "endpoint":"/user/password/change",
               "response":"BEResponse"
            }
         ]
      }
   ],
   "responses":[
      {
         "object-class":"AuthResponse",
         "base-class":"BEResponse",
         "mapping":{
            "data":{
               "property":"user",
               "object-class":"BEUser"
            }
         }
      }
   ]
}
